{
	"name": "breathing-exercise",
	"description": "Take a moment to breathe and relax.",
	"version": "0.0.1",
	"publisher": "canserhat5951",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/canserhat77/vscode-breathing-exercise"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onCommand:breathe.start"
	],
	"contributes": {
		"commands": [
			{
				"command": "breathe.start",
				"title": "Start Breathing Exercise",
				"icon": "$(play-circle)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "breathing-exercise",
					"title": "Breathing Exercise",
					"icon": "media/breath-icon.svg"
				}
			]
		},
		"views": {
			"breathing-exercise": [
				{
					"id": "breathing-exercise-view",
					"name": "Breathing Exercise",
					"type": "webview"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "breathe.start",
					"when": "view == breathing-exercise-view",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -w -p ./",
		"build": "npm run compile  && vsce package"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"eslint": "^9.13.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.11.0"
	}
}